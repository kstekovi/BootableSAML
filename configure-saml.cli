/subsystem=keycloak-saml/secure-deployment=simple-webapp.war/:add

/subsystem=keycloak-saml/secure-deployment=simple-webapp.war/SP="my-sp"/:add(sslPolicy=EXTERNAL,logoutPage="http://localhost:8080/simple")

/subsystem=keycloak-saml/secure-deployment=simple-webapp.war/SP="my-sp"/Key=KEY1:add(signing=true, \
PrivateKeyPem="", \
CertificatePem="")

/subsystem=keycloak-saml/secure-deployment=simple-webapp.war/SP="my-sp"/IDP=idp/:add( \
    SingleSignOnService={ \
        signRequest=true, \
        validateResponseSignature=true, \
        validateAssertionSignature=false, \
        requestBinding=POST, \
        bindingUrl=http://localhost:8180/realms/my-realm/protocol/saml}, \
    SingleLogoutService={ \
        signRequest=true, \
        signResponse=true, \
        validateRequestSignature=true, \
        validateResponseSignature=true, \
        requestBinding=POST, \
        responseBinding=POST, \
        postBindingUrl=http://localhost:8180/realms/my-realm/protocol/saml, \
        redirectBindingUrl=http://localhost:8180/realms/my-realm/protocol/saml} \
)

/subsystem=keycloak-saml/secure-deployment=simple-webapp.war/SP="my-sp"/IDP=idp/:write-attribute(name=signatureAlgorithm,value=RSA_SHA256)

/subsystem=keycloak-saml/secure-deployment=simple-webapp.war/SP="my-sp"/IDP=idp/:write-attribute(name=signatureCanonicalizationMethod,value=http://www.w3.org/2001/10/xml-exc-c14n#)
